select id,
       mb_id,
       first_name,
       last_name,
       email,
       gender,
       birth_date,
       mobile_phone_number,
       address_line1,
       city,
       state,
       postal_code,
       image_url,
       emergency_contact_name,
       emergency_contact_email,
       emergency_contact_relationship,
       emergency_contact_phone,
       send_promotional_emails_flg
from clients
where fitness_network_id = 1361
order by id desc